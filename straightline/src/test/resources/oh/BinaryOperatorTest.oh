// OperatorTest.oh
// tests the diverse binary operators

// Int Operators
print("16-11=", 16-11, ". 2+3=", 2+3, ". 15/3=", 15/3, ". 16/3=", 16/3,
  ". 3*2-1=", 3*2-1, ". 3<4=", 3<4, ". 3<=4=", 3<=4, ". 3<=2=", 3<=2, 
  ". 3==3=", 3==3, ". 3==4=", 3==4, ". 3!=4=", 3!=4, ". 3!=3=", 3!=3, 
  ". 3>3=", 3>3, ". 3>2=", 3>2, ". " );

// Boolean Operators
print("true=", true, ". false=", false, ". !true=", !true, ". !false=", !false,
  ". true==true=", true==true, ". true==false=", true==false, ". true!=true=", true!=true,
  ". true!=false=", true!=false, ". true&true=", true&true, ". true&false=", true&false,
  ". false&false=", false&false,
  ". true|true=", true|true, ". true|false=", true|false, ". false|false=", false|false,
  ". true^true=", true^true, ". false^false=", false^false, ". true^false=", true^false, ". ");

// Double Operators
print("6.6/2=", 6.6 / 2, ". 3.3*2=", 3.3*2, ". 3.4+1.1=", 3.4+1.1, ". 3.7-1.2=", 3.7-1.2 );
print(". 3.3==3.3 ", 3.3==3.3, ". 3.3==17.5 ", 3.3==17.5, ". 3.5<7.6 ", 3.5 < 7.6,
  ". 3.5<=3.5 ", 3.5 <= 3.5, ". 3.5<=17.6 ", 3.5<=17.6, ". 3.95<=1.7125 ", 3.95<=1.7125,
  ". 3.5>2 ", 3.5>2, ". 3.5>=3.5 ", 3.5>=3.5, ". 3.5>=1.2 ", 3.5>=1.2, ". 3.5>=17.0 ", 3.5>=17.0,
  ". 3.5!=3.5=", 3.5!=3.5, ". 3.5!=35=", 3.5!=35, ". " );

// Text Operators
print("Hello==Hello ", "Hello"=="Hello", ". Hello!=Hello ", "Hello"!="Hello", ". Hello!=World ",
  "Hello"!="World", ". Hello,+World!= ", "Hello, "+"World!" );

